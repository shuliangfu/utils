{
  "name": "@dreamer/utils",
  "version": "1.0.0-beta.2",
  "license": "MIT",
  "exports": {
    "./array": "./src/array.ts",
    "./string": "./src/string.ts",
    "./object": "./src/object.ts",
    "./date": "./src/date.ts",
    "./number": "./src/number.ts",
    "./async": "./src/async.ts",
    "./url": "./src/url.ts",
    "./format": "./src/format.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.4"
  }
}
