{
  "name": "@dreamer/utils",
  "version": "1.0.0-beta.5",
  "license": "MIT",
  "compilerOptions": {
    "lib": [
      "deno.window",
      "dom"
    ]
  },
  "exports": {
    "./array": "./src/array.ts",
    "./string": "./src/string.ts",
    "./object": "./src/object.ts",
    "./date": "./src/date.ts",
    "./number": "./src/number.ts",
    "./async": "./src/async.ts",
    "./lock": "./src/lock.ts",
    "./system": "./src/system.ts",
    "./url": "./src/url.ts",
    "./format": "./src/format.ts",
    "./file": "./src/file.ts",
    "./client/array": "./src/client/array.ts",
    "./client/string": "./src/client/string.ts",
    "./client/object": "./src/client/object.ts",
    "./client/date": "./src/client/date.ts",
    "./client/number": "./src/client/number.ts",
    "./client/async": "./src/client/async.ts",
    "./client/url": "./src/client/url.ts",
    "./client/format": "./src/client/format.ts",
    "./client/file": "./src/client/file.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.6",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.0-beta.4"
  }
}
