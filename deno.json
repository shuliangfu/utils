{
  "name": "@dreamer/utils",
  "version": "1.0.0",
  "license": "MIT",
  "exports": {
    "./array": "./src/array.ts",
    "./string": "./src/string.ts",
    "./object": "./src/object.ts",
    "./date": "./src/date.ts",
    "./number": "./src/number.ts",
    "./async": "./src/async.ts",
    "./lock": "./src/lock.ts",
    "./system": "./src/system.ts",
    "./url": "./src/url.ts",
    "./format": "./src/format.ts",
    "./file": "./src/file.ts",
    "./validator": "./src/validator.ts",
    "./client/array": "./src/client/array.ts",
    "./client/string": "./src/client/string.ts",
    "./client/object": "./src/client/object.ts",
    "./client/date": "./src/client/date.ts",
    "./client/number": "./src/client/number.ts",
    "./client/async": "./src/client/async.ts",
    "./client/url": "./src/client/url.ts",
    "./client/format": "./src/client/format.ts",
    "./client/file": "./src/client/file.ts",
    "./client/validator": "./src/client/validator.ts",
    "./client/clipboard": "./src/client/clipboard.ts",
    "./client/http": "./src/client/http/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "docs/**/*.md",
      "LICENSE.md",
      "README.md",
      "TEST_REPORT.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.0",
    "pako": "npm:pako@2.1.0"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "lib": [
      "deno.window",
      "dom"
    ]
  }
}
